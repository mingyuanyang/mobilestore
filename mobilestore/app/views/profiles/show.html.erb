<p id="notice">
  <% notice %></p>
<div id="profile_show">
  <br/>

  <p>
    <b>Firstname:</b>
    <%= @profile.firstname %>
  </p>

  <p>
    <b>Lastname:</b>
    <%= @profile.lastname %>
  </p>

  <p>
    <b>Address:</b>
    <%= @profile.address %>
  </p>

  <p>
    <b>Mobile:</b>
    <%= @profile.mobile %>
  </p>

  <br/>


  <%= link_to 'Edit', edit_profile_path(@profile) %> |
  <%= link_to 'Back', profiles_path %>
</div>